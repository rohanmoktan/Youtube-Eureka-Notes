{
    "name":"Rohan's Yt ext",
    "version":"0.0.1",
    "description":"My time stamps and playlist total time calcutlator",
    "permissions":["storage","tabs"],
    "host_permissions":["https://*.youtube.com/*"],
    "background":{
        "service_worker":"background.js"
    },
    "content_scripts":[
        { 
            "matches":["https://*.youtube.com/*"],
            "js": ["contentScript.js"]
        }
    ],
    "web_accessible_resources":[
        {
            "resources": [
                "assets/bookmark.png",
                "assets/play.png",
                "assets/delete.png",
                "assets/save.png"
            ],
            "matches":["https://*.youtube.com/*"]
        }
    ],
    "action":{
        "default_icon":{
            "16":"assets/ext-icon2.png",
            "24": "assets/ext-icon2.png",
            "32": "assets/ext-icon2.png"
        },
        "default_title": "My YT Bookmarks",
        "default_popup": "popup.html"
    },
    "manifest_version":3

    
}